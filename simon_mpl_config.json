{
    "figsize": [12, 10],
    "cms_label" : "Work in Progress",
    "ratiopad" : {
        "height" : 0.5,
        "hspace" : 0.0,
        "auto_ylim" : {
            "padding" : 0.15,
            "percentile" : 0.99,
            "min_threshold" : 0.2
        }
    },
    "axis_labels": {
        "Zpt" : "$p_{T}^{Z}$ [GeV]",
        "Zy" : "$y^{Z}$",
        "Zmass" : "$m_{Z}$ [GeV]",
        "genHT" : "$H_{T}^{gen}$ [GeV]",
        "nTrueInt" : "$N_{vtx}^{true}$",
        "nPV" : "$N_{vtx}^{reco}$",
        "rho" : "$\\rho$",
        "MET" : "$E_{T}^{miss}$ [GeV]",
        "numLooseB" : "$N_{b}^{loose}$",
        "numMediumB" : "$N_{b}^{medium}$",
        "numTightB" : "$N_{b}^{tight}$",
        "numJets" : "$N_{jets}$",
        "leadMuPt" : "$p_{T}^{\\mu_{lead}}$ [GeV]",
        "leadMuEta" : "$\\eta^{\\mu_{lead}}$",
        "leadMuCharge" : "$Q^{\\mu_{lead}}$",
        "subMuPt" : "$p_{T}^{\\mu_{sublead}}$ [GeV]",
        "subMuEta" : "$\\eta^{\\mu_{sublead}}$",
        "subMuCharge" : "$Q^{\\mu_{sublead}}$"
    },
    "auto_logx_patterns" : [
        "*pt",
        "MET",
        "*HT"
    ]
}